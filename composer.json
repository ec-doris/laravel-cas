{
    "name": "ec-doris/laravel-cas",
    "description": "A Laravel CAS authentication bundle for EU Login and EC applications with automatic configuration and route publishing.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "authentication",
        "cas",
        "central authentication service",
        "single sign-on",
        "sso",
        "laravel",
        "eu-login",
        "european-commission"
    ],
    "homepage": "https://github.com/ec-doris/laravel-cas",
    "support": {
        "issues": "https://github.com/ec-doris/laravel-cas/issues",
        "source": "https://github.com/ec-doris/laravel-cas"
    },
    "authors": [
        {
            "name": "EC DORIS Team",
            "homepage": "https://github.com/ec-doris"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ecphp/cas-lib": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^9 || ^10 || ^11 || ^12",
        "loophp/psr17": "^1.0",
        "nyholm/psr7": "^1.8",
        "symfony/cache": "^6.4 || ^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.26 || ^9.4",
        "roave/security-advisories": "dev-latest",
        "symfony/psr-http-message-bridge": "^6.4 || ^7.1"
    },
    "suggest": {
        "symfony/psr-http-message-bridge": "To bridge between Laravel and PSR HTTP Message"
    },
    "autoload": {
        "psr-4": {
            "EcDoris\\LaravelCas\\": "src/",
            "EcDoris\\LaravelCas\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {}
    },
    "extra": {
        "laravel": {
            "providers": [
                "EcDoris\\LaravelCas\\Providers\\AppServiceProvider"
            ]
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit"
    }
}